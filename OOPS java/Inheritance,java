class Circle{
    int radius;
    
    Circle(int r){
        this.radius=r;
    }
    
    void Area(){
        System.out.println(3.14*Math.pow(radius,2));
    }
}

class Circum extends Circle{
    Circum(int radius){
        super(radius);
    }
    void circumference(){
        System.out.println(2*3.14*radius);
    }
}

class Inheritance{
    public static void main(String args[]){
        Circum c=new Circum(10);
        c.Area();
        c.circumference();
    }
}

